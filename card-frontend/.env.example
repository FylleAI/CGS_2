# Card Frontend Configuration

# API Configuration
# Card Service runs on port 8001 (separate from main API on port 8000)
VITE_API_BASE_URL=http://localhost:8001
VITE_CARD_API_URL=http://localhost:8001/api/v1/cards

# Tenant ID for testing (can be overridden via URL parameter ?tenant_id=...)
VITE_TENANT_ID=test-user@example.com

# App Configuration
VITE_APP_NAME=Card Manager
VITE_APP_VERSION=1.0.0

# Feature Flags
VITE_ENABLE_RELATIONSHIPS=true
VITE_ENABLE_EDITING=true
VITE_ENABLE_EXPORT=false

